@page
@model hakims_livs.Pages.IndexModel
@{
    ViewData["Title"] = "Hakim´s Livs";
}

<h1 class="title">Välkommen till Hakim´s Livs</h1>
<div class="product-grid">

    @foreach (var item in Model.Product)
    {
        <div class="card card-product single-result">
            <img src="data:image/jpeg;base64," class="card-img-product" alt="...">
            <div class="card-body card-product-description">
                <h4 class="card-title">@Html.DisplayFor(modelItem => item.Name)</h4>
                <p class="card-text">@Html.DisplayFor(modelItem => item.Description)</p>
            </div>
            <div class="card-product-footer">
                <h5 class="card-title">@Html.DisplayFor(modelItem => item.Price)kr</h5>

                <a href="#" class="btn btn-primary btn-product">Köp</a>
            </div>
        
            </div>
    }
</div>